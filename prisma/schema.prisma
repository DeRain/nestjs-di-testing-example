generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Query {
  id        String   @id @default(cuid())
  address   String
  chainId   Int
  resultWei String
  queriedAt DateTime @default(now())

  @@index([address, queriedAt])
}
